<div class="container">
  <div class="row">
  	<h1>マイページ</h1>
    <div style="padding: 5px; margin-top: 20px; margin-bottom: 10px; border: 1px solid #3cb371; border-radius: 10px;">
      <table class="table table-striped">
        <tbody>
        	<%= attachment_image_tag @enduser, :profile_image, :fill, 100, 100, class: "img-circle profile-thumb", fallback: "no_image.jpg"%>
          <tr style="background-color:#f5f5f5">
	          <th><p>お名前</p></th>
	          <th><p>メールアドレス</p></th>
	      </tr>
	      <tr style="background-color:#f5f5f5">
	          <th><%= @enduser.name %></th>
      	      <th><%= @enduser.email %></th>
          </tr>
        </tbody>
      </table>
      <% if admin_signed_in? %>
       <div class="field">
        <%= link_to "強制退会", admin_path, method: :delete, data: { confirm: 'Are you sure?' }, class: "pull-denger", class: "btn btn-danger btn-xs" %>
       </div>
      <% end %>
    </div>
  </div>
</div>